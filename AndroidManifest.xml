<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.RunningWheel.everytour"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission 
        android:name="android.permission.INTERNET"
        />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name="com.RunningWheel.everytour.LogoScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.RunningWheel.everytour.MainScreen" />
        <activity 
            android:name="com.RunningWheel.everytour.Signup"></activity>
        <activity android:name="com.RunningWheel.everytour.NotSignedIn"></activity>
    </application>

</manifest>
